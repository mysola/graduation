<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wangyang.mapper.AnonymousUrlClickDao">

    <select id="getUrlClick" resultType="AnonymousUrlClick">
        select url , click_count as anonymousClick from doc_url where  url = #{url}
    </select>

</mapper>